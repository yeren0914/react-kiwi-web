{
    "compilerOptions": {
        "target": "es2024",
        "lib": ["dom", "esnext"],
        "module": "esnext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": true,
        "jsx": "react-jsx",
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
}
